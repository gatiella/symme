*.zip
*.tar.gz
*.rar
*.7z

# Small temp files
*.class
*.log
*.pyc
*.swp
.DS_Store
.buildlog/
.history

# Keep essential files for collaboration:
# .flutter-plugins-dependencies
# pubspec.yaml
# pubspec.lock
