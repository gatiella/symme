# Remove already-tracked large files and prevent future ones

# Critical: Remove large build artifacts
.dart_tool/
build/
**/build/
android/.gradle/
android/app/build/
android/app/debug/
android/app/profile/
android/app/release/
android/app/intermediates/
android/app/outputs/
ios/build/
ios/Pods/
macos/build/

# Compiled files that get huge
**/*.so
**/*.apk
**/*.ipa
**/*.app
**/*.dill
*.aab

